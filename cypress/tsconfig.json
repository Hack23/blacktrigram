{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom", "es2015"],
    "types": ["cypress", "node"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": ["**/*.ts"]
}
